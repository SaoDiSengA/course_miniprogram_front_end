/* pages/assessment/assessment.wxss */
.addStudent{
  margin-top: 30rpx;
}
.addStudent button{
  width: 680rpx!important ;
  height: 89rpx!important;
  padding-left: 16%;
  color: #469ffa;
  font-size: 32rpx;
  line-height: 50rpx;
  border: 2rpx dotted #3987fa;
  border-radius: 60rpx;
  background: url(https://i.loli.net/2021/10/14/MoJHW9eIlXuhPzs.png) no-repeat 38% center;
  background-size: 8%;
}
.swiper-tab {
  width: 680rpx;
  margin: 0 auto;
  text-align: center;
  height: 68rpx;
  line-height: 68rpx;
  display: flex;
  flex-flow: row;
  justify-content: space-between;
  background-color: #f9f9f9;
  border: 1px solid #f9f9f9;
  border-radius: 50rpx;
}

.swiper-tab-item {
  width: 30%;
  color: #a2a2a2;
  font-size: 28rpx;
  background-color: #f9f9f9;
  border-radius: 50rpx;
}

.active {
  color: #333333;
  font-weight: 700;
  background-color: #ffffff;
  border-radius: 50rpx;
}
swiper {
  height: 1500px;
  display: flex;
}
.list_container{
  display: flex;
  flex-wrap: wrap;
  /* border: 3px solid #434343; */

}
.all{
  padding-top: 200rpx;
  text-align: center;
}
.all image{
  width: 489rpx;
  height: 449rpx;
  margin: auto;
}